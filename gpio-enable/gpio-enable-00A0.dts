/dts-v1/;
/plugin/;

/{
	compatible = "ti,beaglebone", "ti,beaglebone-black";
	part_number = "gpio-enable";
	version = "00A0";

	fragment@0 {
		target = <&am33xx_pinmux>;

		__overlay__ {
			gpio_enable: pinmux_gpio_enable {
				pinctrl-single,pins = <	0x00 0x7
							0x04 0x7
							0x08 0x7
							0x0c 0x7
							0x10 0x7
							0x14 0x7
							0x18 0x7
							0x1c 0x7
							0x30 0x7
							0x34 0x7
							0x38 0x7
							0x3c 0x7
							0x8c 0x7
							0x90 0x7
							0x94 0x7
							0x98 0x7
							0x9c 0x7
							0xc0 0x7
							0xd0 0x27
							0xd4 0x27
							0xd8 0x27
							0xdc 0x27
							0x20 0x27
							0x24 0x27
							0x40 0x27
							0x44 0x27
							0x48 0x27
							0x4c 0x27
							0x78 0x27
							0x7c 0x27
				>;
			};
		};
	};

	fragment@1 {
                target = <&ocp>;

                __overlay__ {
                        gpio_enable_pinmux {
                                compatible = "bone-pinmux-helper";
                                status = "okay";
                                pinctrl-names = "default";
                                pinctrl-0 = <&gpio_enable>;
                        };
                };
        };

};
